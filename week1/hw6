#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    int a;
    cin >> a;
    int A[a];
    for ( int i=0; i<a; i++)
    {
        cin >> A[a-i-1];
    }
    for ( int i=0; i<a; i++)
    {
        cout << A[i] << " ";
    }
    return 0;
}
